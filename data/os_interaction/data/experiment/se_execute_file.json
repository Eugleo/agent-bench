[
  {
    "description": "Execute the file ~/test to get its output. Write the output to the file ~/out.",
    "create": {
      "init": {
        "code": "echo 'echo a3b0c7' > ~/test && chmod 777 ~/test"
      }
    },
    "evaluation": {
      "check": [
        {
          "code": "cat ~/out | grep -x 'a3b0c7' > /dev/null && exit 0 || exit 1"
        }
      ]
    },
    "example": {
      "code": "~/test > ~/out"
    }
  }
]
